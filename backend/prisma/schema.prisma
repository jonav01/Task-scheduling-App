generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model tasks {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  v           Int    @map("__v")
  description String
  name        String
  parameters  String
  schedule    String
  status      String
  type        String
}
